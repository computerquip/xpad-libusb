 ARGS = '-pipe' '-std=gnu99' '-Wall' '-Winvalid-pch' '-g' '-I/usr/include/libusb-1.0' '-Ixpad360.dir'

build xpad360: c_LINKER xpad360.dir/xpad360_threadpool.c.o xpad360.dir/xpad360_usb.c.o
 LINK_ARGS = '-lusb-1.0' '/usr/lib/libpthread.so'
